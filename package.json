{
  "name": "ffmpeg_front",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://edelgarat.github.io/ffmpeg_js_ui/",
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "ramda": "^0.26.1",
    "raw-loader": "^4.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/ react-scripts build",
    "build:gh-pages": "PUBLIC_URL=/ffmpeg_js_ui react-scripts build",
    "mv-build-to-docs": "mv build docs",
    "serve": "serve build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write --list-different \"src/**/*.{ts,tsx}\""
  },
  "eslintConfig": {
